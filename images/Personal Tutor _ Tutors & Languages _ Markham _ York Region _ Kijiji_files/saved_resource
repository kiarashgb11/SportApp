<session-props-provider inline-template>
    <session-expired inline-template
                     v-cloak
                     :session-out-public-redirect="sessionOutPublicRedirect"
                     :session-out-private-redirect="sessionOutPrivateRedirect"
                     :session-enabled="sessionEnabled"
                     :is-authenticated="isAuthenticated"
                     :page-editor="pageEditor"
                     :funnel-in-progress="funnelInProgress"
                     :session-waiting-buffer="sessionWaitingBuffer">
        <global-modal ref="session-modal"
                      id="session-modal"
                      no-close-on-backdrop
                      no-close-button>

            <template #heading>
                Are you still there?
            </template>

            <template #body>
                <div class="temp-data-warning-modal__message">
                    <div v-if="!isSessionExpired">
                        <p>It seems you've stepped away! We'll end your session in&nbsp;{{remaining}} seconds, unless you'd rather we didn't.</p>
                    </div>
                    <div v-else>
                        <p>Your session has ended. Sorry! </p>
<p>Redirecting in {{redirection}} seconds...</p>
                    </div>
                </div>
            </template>


            <template #primary-button>
                <onlia-button v-if="!isSessionExpired"
                              tag="button"
                              type="button"
                              label="Continue session"
                              @click="retainSession">
                </onlia-button>

                <onlia-button v-else
                              tag="button"
                              type="button"
                              label="Close"
                              @click="expireSession">
                </onlia-button>
            </template>
        </global-modal>
    </session-expired>
</session-props-provider>



