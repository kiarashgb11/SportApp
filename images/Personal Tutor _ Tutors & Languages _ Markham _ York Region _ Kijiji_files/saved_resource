<!-- Begin Usabilla for Websites screenshot code -->
<script>window.usabilla_live("setEventCallback", function (category, action, label, value, eventdata) {
    //console.log(action);
	if ( action == 'undefined' ) {
        return;
    }

    if ( action != "Feedback:Open" ) {
        return
    }
	
	const header = document.querySelectorAll('.header')[0];
    const chatbot = document.querySelector('[data-cxco-dci]');

    if ( action = "Feedback:Open" ) {
        //MODIFY CODE TO HIDE SPECIFIC ELEMENT HERE
        header.style.display = "none";
		if(chatbot){
			chatbot.style.display = "none";
		}
    }

    setTimeout(function () {
        //CODE TO DISPLAY ELEMENT
        header.style.display = "block";
		if(chatbot){
			chatbot.style.display = "block";
		}
    }, 1200);
});</script>
<!-- End Usabilla for Websites screenshot code -->

